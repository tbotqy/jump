<% content_for :javascript_include_tags do %>
  <%= javascript_include_tag "modules/page_title", "modules/global_header", "modules/dashbord", "modules/tweet_card", "modules/read_more" %>
  <%= javascript_include_tag "statuses/sent_tweets" %>
<% end %>
<% content_for :global_header do %>
  <%= render "shared/global_header",{ :current_user => @current_user} %>
<% end %>
<%= render "shared/dashbord" %>
<%= render "shared/adsense/pc/user_timeline/above_timeline" %>
<!-- #wrap-timeline-lower -->
<div id="wrap-timeline-lower">
  <!-- #wrap-main -->
  <div id="wrap-main">
    <%= render "shared/timeline", {:statuses => @statuses, :has_next => @has_next, :current_user => @current_user, :oldest_tweet_id => @oldest_tweet_id} %>
  </div>
  <!-- /#wrap-main -->
</div>
<!-- /#wrap-timeline-lower -->
<%= render "shared/adsense/pc/user_timeline/under_timeline" %>
