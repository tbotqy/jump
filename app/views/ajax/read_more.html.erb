<% if @statuses %>
  <%= render "shared/each_status", {statuses: @statuses, current_user: current_user, oldest_tweet_id: @oldest_tweet_id} %>
  <% if @has_next %>
    <%= render "shared/read_more", {has_next: @has_next} %>
  <% else %>
    <%= render "shared/no_more_status" %>
  <% end %>
<% else %>
  <%= render "shared/no_status" %>
<% end %>
